{
  "name": "bot-agent-backend",
  "version": "1.0.0",
  "description": "Bot Agent Platform Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "pm2:start": "npm run build && pm2 start ../ecosystem.config.js",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup",
    "pm2:stop": "pm2 stop bot-agent-platform",
    "pm2:restart": "npm run build && pm2 restart bot-agent-platform",
    "pm2:logs": "pm2 logs bot-agent-platform",
    "pm2:status": "pm2 status",
    "encoding:check": "node scripts/encoding-preprocess.js",
    "encoding:fix": "node scripts/encoding-preprocess.js --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "bot",
    "ai",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/node-schedule": "^2.1.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cron-parser": "^5.5.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "iconv-lite": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "node-schedule": "^2.1.1",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.3",
    "prisma": "^5.22.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
